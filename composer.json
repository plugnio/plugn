{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2-httpclient": "dev-master",
        "yiisoft/yii2": "~2.0.14",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "kartik-v/yii2-widget-timepicker": "dev-master",
        "cloudinary/cloudinary_php": "^1.16",
        "notamedia/yii2-sentry": "^1.3",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "openbuildings/swiftmailer-css-inliner": "*",
        "wbraganca/yii2-dynamicform": "dev-master",
        "yiisoft/yii2-shell": "^2.0",
        "leandrogehlen/yii2-treegrid": "*",
        "ajaxray/short-code": "^1.1",
        "liyunfang/yii2-widget-linkpager": "^1.0",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "sentry/sdk": "2.1.0",
        "understeam/yii2-slack": "v0.3.0",
        "yiier/yii2-slack": "*",
        "moonlandsoft/yii2-phpexcel": "*",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-widget-touchspin": "dev-master",
        "kartik-v/yii2-widget-rangeinput": "dev-master",
        "lubosdz/yii2-ui-range-plus-minus": "^1.1",
        "bizley/quill": "^3.1",
        "twitf/yii2-dropzone": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "janisto/yii2-timepicker": "*",
        "softark/yii2-dual-listbox": "dev-master",
        "maksyutin/yii2-dual-list-box": "dev-master",
        "ssimpson/yii2-dual-list-box": "dev-master",
        "borales/yii2-phone-input": "*",
        "segmentio/analytics-php": "^1.5",
        "mixpanel/mixpanel-php" : "^2.10",
        "yiisoft/yii2-redis": "~2.0.0",
        "aws/aws-sdk-php": "^3.183",
        "stripe/stripe-php": "^10.6"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "4.0.x-dev | ^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "phpunit/phpunit": "~5.7.27 || ~6.5.5",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/specify": "~0.4.6",
        "codeception/module-rest": "^2.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
